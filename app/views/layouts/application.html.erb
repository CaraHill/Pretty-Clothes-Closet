<!DOCTYPE html>
<html>
  <head>
    <title>Pretty Clothes Closet</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="site-main">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <h1 class="page-header">All of the Products</h1>
          </div>
          <p class="notice"><%= notice %></p>
          <p class="alert"><%= alert %></p>

          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
